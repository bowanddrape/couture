{
  "author": "graphicsforge",
  "scripts": {
    "build_js": "mkdir -p public/dist && ./node_modules/browserify/bin/cmd.js --exclude ./model/* -t [ babelify --presets [ react es2015 ] ] -r ./views/BowAndDrape.jsx:BowAndDrape -o public/dist/BowAndDrape.js",
    "build_sprites": "./node_modules/.bin/sprity create public/dist/ public/*.png -c / -p less -s sprite.less --cachebuster",
    "build_styles": "/usr/bin/lessc ./styles/styles.less public/dist/styles.css",
    "build": "npm run build_js && npm run build_sprites && npm run build_styles",
    "start": "npm run build && sudo node server.js"
  },
  "dependencies": {
    "async": "~1.5.2",
    "aws-sdk": "^2.7.20",
    "babel-preset-es2015": "~6.16.0",
    "babel-preset-react": "~6.16.0",
    "babelify": "~7.3.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "browserify": "^13.0.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "~4.13.4",
    "html-pdf": "^2.1.0",
    "jsonwebtoken": "^7.1.9",
    "jwt-decode": "^2.1.0",
    "less-vars-to-js": "^1.1.2",
    "multer": "^1.2.0",
    "multer-s3": "^2.5.0",
    "mysql": "^2.13.0",
    "newrelic": "^1.33.0",
    "node-babel": "^0.1.2",
    "node-jsx-babel": "^0.14.2",
    "pg": "^6.1.0",
    "pg-native": "^1.10.0",
    "react": "~15.3.2",
    "react-autocomplete": "^1.4.0",
    "react-swipeable": "^3.6.0",
    "sendgrid": "^4.7.1",
    "stripe": "^4.15.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.16.0",
    "browserify": "~13.0.0",
    "react-dom": "~15.3.2",
    "sprity-cli": "^1.0.1",
    "sprity-less": "^1.0.3",
    "webpack": "^1.13.2"
  }
